{
    "name": "image-viewer",
    "displayName": "image viewer",
    "icon": "resource/sawtooth_2_sin_3.png",
    "description": "c++插件，支持查看自定义的图像数据结构",
    "version": "1.1.8",
    "publisher": "ZouJun",
    "author": {
        "name": "ZouJun"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/matchman-zj/vscode-image-viewer.git"
    },
    "engines": {
        "vscode": "^1.102.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension.js",
    "extensionKind": [
        "ui"
    ],
    "activationEvents": [
        "onCommand:image-viewer.showImage"
    ],
    "contributes": {
        "commands": [
            {
                "command": "image-viewer.showImage",
                "title": "show tiff image"
            }
        ],
        "menus": {},
        "configuration": {
            "title": "image viewer Config",
            "properties": {
                "image-viewer.ShowImgCmd": {
                    "type": "string",
                    "default": "",
                    "description": "查看图像命令"
                },
                "image-viewer.ShowImgArgs": {
                    "type": "string",
                    "default": "",
                    "description": "查看图像命令的参数，为空则默认为 保存临时图像的路径"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.102.0",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.25.1",
        "typescript": "^5.8.3"
    }
}